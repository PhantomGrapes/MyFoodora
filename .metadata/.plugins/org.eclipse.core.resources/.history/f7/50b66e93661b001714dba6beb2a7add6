	// new change
	private MyFoodora myFoodora;

// Constructors of User
	public User(){
		
	}
	
// Register/Unregister to system
	public void registerToSystem(){
		
	}
	
	public void unregisterToSystem(){
		
	}

	public String getName() {
		return name;
	}

